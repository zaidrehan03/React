          var custom_styleMobile = {
            'top': '60px'
        };
        Object.assign(eleMobile.style, custom_styleMobile);
       }
  console.log("test header push down");
}
});
  
    webengage.notification.onClose(function(data){
if(data.notificationId === "~10cb43a86" ){
